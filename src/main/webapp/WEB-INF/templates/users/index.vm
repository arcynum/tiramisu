<table>
	<thead>
		<tr>
			<th>ID</th>
			<th>Username</th>
			<th>Email</th>
			<th>Password</th>
			<th>Salt</th>
			<th>Active</th>
			<th>Created</th>
			<th>Modified</th>
		</tr>
	</thead>
	<tbody>
		#foreach($user in $users)
		<tr>
			<td>$!user.getId()</td>
			<td>$!user.getUsername()</td>
			<td>$!user.getEmail()</td>
			<td>$!user.getPassword()</td>
			<td>$!user.getSalt()</td>
			<td>$!user.getActive()</td>
			<td>$!user.getCreated()</td>
			<td>$!user.getModified()</td>
		</tr>
		#end
	</tbody>
</table>