<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="au.com.ifti.models">
	<class name="ReportModel" table="dlreport">
		<id name="id" column="id">
			<generator class="identity" />
		</id>
		<property name="name" />
		<property name="description" />
		<property name="created" />
		<property name="modified" />
		
		<property name="creator" />
		<property name="link" type="string" />
		<property name="platform" />
		
		<property name="environment" />
		<property name="reportType" />
		<property name="fileName" />
		<property name="reportPath" />
		<property name="program" />
		<property name="storedProcessName" />
		<property name="schedule" length="300"/>
		<property name="additionalInfo" length="1024"/>
		
		<set name="tags" table="dltags_dlreports">
			<key column="report_id" />
			<many-to-many class="au.com.ifti.models.TagModel" column="tag_id" />
		</set>
		
	</class>
</hibernate-mapping>

