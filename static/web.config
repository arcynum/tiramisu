<?xml version="1.0" encoding="UTF-8"?>
<configuration>
    <system.webServer>
        <rewrite>
            <rules>
                <rule name="Tomcat">
                    <match url="(tomcat.+)" />
                    <action type="Rewrite" url="http://localhost:8080/Tiramisu/{R:0}" />
                </rule>
            </rules>
        </rewrite>
    </system.webServer>
</configuration>
